{
  "name": "relay",
  "productName": "Relay",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/main/index.js",
  "description": "Relay - Modern Data Dashboard",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win --config electron-builder.yml",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "build:renderer": "vite build --config vite.renderer.config.ts",
    "preview:renderer": "npm run build:renderer && vite preview --config vite.renderer.config.ts --host --port 4173",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:renderer": "vitest run -c vitest.renderer.config.ts"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "playwright": "^1.57.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^22.5.3",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.3.2",
    "electron": "^32.1.2",
    "electron-builder": "^26.0.12",
    "electron-vite": "^2.3.0",
    "jsdom": "^27.3.0",
    "typescript": "^5.9.3",
    "vite": "^5.3.5",
    "vitest": "^4.0.15"
  }
}
