{
  "name": "relay",
  "productName": "Relay",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/main/index.js",
  "description": "Relay - Modern Data Dashboard",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:win": "npm run build && electron-builder --win --config electron-builder.yml",
    "build:mac": "npm run build && electron-builder --mac --config electron-builder.yml",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "build:renderer": "vite build --config vite.renderer.config.ts",
    "dev:test": "vite --config vite.renderer.config.ts --port 4173",
    "preview:renderer": "npm run build:renderer && vite preview --config vite.renderer.config.ts --host --port 4173",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:renderer": "vitest run -c vitest.renderer.config.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "chokidar": "^3.6.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "gridstack": "^12.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.5.3",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.7.0",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "vitest": "^4.0.15"
  }
}
